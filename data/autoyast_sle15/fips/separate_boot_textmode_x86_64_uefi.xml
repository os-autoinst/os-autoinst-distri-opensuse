      <pattern>x11_yast</pattern>
      <pattern>yast2_basis</pattern>
    </patterns>
    <products t="list">
      <product>SLES</product>
    </products>
  </software>
  <ssh_import t="map">
    <copy_config t="boolean">false</copy_config>
    <import t="boolean">false</import>
  </ssh_import>
  <suse_register t="map">
    <addons t="list">
      <addon t="map">
        <arch>{{ARCH}}</arch>
        <name>sle-module-server-applications</name>
        <reg_code/>
        <release_type>nil</release_type>
        <version>{{VERSION}}</version>
      </addon>
      <addon t="map">
        <arch>{{ARCH}}</arch>
        <name>sle-module-basesystem</name>
        <reg_code/>
        <release_type>nil</release_type>
        <version>{{VERSION}}</version>
      </addon>
    </addons>
    <do_registration t="boolean">true</do_registration>
    <email/>
    <install_updates t="boolean">false</install_updates>
    <reg_code>{{SCC_REGCODE}}</reg_code>
    <reg_server>{{SCC_URL}}</reg_server>
    <slp_discovery t="boolean">false</slp_discovery>
  </suse_register>
  <tftp-server t="map">
    <start_tftpd t="boolean">false</start_tftpd>
  </tftp-server>
  <timezone t="map">
    <timezone>America/New_York</timezone>
  </timezone>
  <user_defaults t="map">
