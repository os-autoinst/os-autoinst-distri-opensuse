From 5ea5e6f8714119c706ce75238c37be2e4d543cab Mon Sep 17 00:00:00 2001
From: Nalin Dahyabhai <nalin@redhat.com>
Date: Mon, 12 Jan 2026 11:37:52 -0500
Subject: [PATCH 09/10] bud cache add-copy-chown: go local

Instead of ADDing data from a remotely-hosted web server during the
test, bring up a very minimal one on a local http server and download
from it as part of an ADD instruction.

Signed-off-by: Nalin Dahyabhai <nalin@redhat.com>
---
 tests/bud.bats                        | 6 +++++-
 tests/bud/cache-chown/Dockerfile.url1 | 3 ++-
 tests/bud/cache-chown/Dockerfile.url2 | 3 ++-
 3 files changed, 9 insertions(+), 3 deletions(-)

diff --git a/tests/bud.bats b/tests/bud.bats
index 38d085c6b70..3e55b350223 100644
--- a/tests/bud.bats
+++ b/tests/bud.bats
@@ -5669,6 +5669,10 @@ EOF
 }
 
 @test "bud cache add-copy-chown" {
+  contentdir=${TEST_SCRATCH_DIR}/content
+  mkdir -p ${contentdir}
+  echo sure, this counts as a readme file > ${TEST_SCRATCH_DIR}/content/README.md
+  starthttpd $contentdir
   # Build each variation of COPY (from context, from previous stage) and ADD (from context, not overriding an archive, URL) twice.
   # Each second build should produce an image with the same ID as the first build, because the cache matches, but they should
   # otherwise all be different.
@@ -5679,7 +5683,7 @@ EOF
       iidfile=${TEST_SCRATCH_DIR}/${action}${i}
       containerfile=Dockerfile.${action}$(((i-1) % 2 + 1))
 
-      run_buildah build --iidfile $iidfile --layers --quiet $WITH_POLICY_JSON -f $containerfile $BUDFILES/cache-chown
+      run_buildah build --build-arg=HTTP_SERVER_PORT=${HTTP_SERVER_PORT} --iidfile $iidfile --layers --quiet $WITH_POLICY_JSON -f $containerfile $BUDFILES/cache-chown
     done
   done
 
diff --git a/tests/bud/cache-chown/Dockerfile.url1 b/tests/bud/cache-chown/Dockerfile.url1
index 9c1420470f0..e77a8d85c03 100644
--- a/tests/bud/cache-chown/Dockerfile.url1
+++ b/tests/bud/cache-chown/Dockerfile.url1
@@ -1,2 +1,3 @@
 FROM scratch
-ADD --chown=1:1 https://raw.githubusercontent.com/containers/buildah/main/LICENSE /
+ARG HTTP_SERVER_PORT
+ADD --chown=1:1 http://0.0.0.0:${HTTP_SERVER_PORT}/README.md /
diff --git a/tests/bud/cache-chown/Dockerfile.url2 b/tests/bud/cache-chown/Dockerfile.url2
index 0ba353eddfc..7bd2d107f28 100644
--- a/tests/bud/cache-chown/Dockerfile.url2
+++ b/tests/bud/cache-chown/Dockerfile.url2
@@ -1,2 +1,3 @@
