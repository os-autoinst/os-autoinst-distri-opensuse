+		}
+		handlerCmd[0] = abs
+	}
+	envSlice := strings.Split(envList, ":")
+	var envs, inherits []string
+	for _, env := range envSlice {
+		if strings.Contains(env, "=") {
+			envs = append(envs, env)
+		} else {
+			inherits = append(inherits, env)
+		}
