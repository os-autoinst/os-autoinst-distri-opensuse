  <scripts>

    <chroot-scripts config:type="list">
      <script>
        <filename>3_chroot.sh</filename>
        <interpreter>shell</interpreter>
        <location/>
        <feedback config:type="boolean">false</feedback>
	<feedback_type>message</feedback_type>
        <source><![CDATA[#!/bin/sh
tee YaST2-Second-Stage.service <<EOT
[Unit]
Description=YaST2 Second Stage
After=apparmor.service local-fs.target plymouth-start.service
Before=systemd-user-sessions.service network.service NetworkManager.service SuSEfirewall2_init.service SuSEfirewall2.service
ConditionPathExists=/var/lib/YaST2/runme_at_boot

[Service]
Type=oneshot
Environment=SYSTEMCTL_OPTIONS=--ignore-dependencies TERM=linux
# Turn off plymouth completely before accessing tty
ExecStartPre=-/usr/bin/plymouth quit
ExecStart=/usr/lib/YaST2/startup/YaST2.Second-Stage
RemainAfterExit=yes
TimeoutSec=0
ExecStartPost=/usr/bin/rm -f /var/lib/YaST2/runme_at_boot
ExecStartPost=/usr/bin/systemctl restart systemd-vconsole-setup.service
StandardInput=tty

[Install]
WantedBy=default.target

EOT

cp YaST2-Second-Stage.service /mnt/usr/lib/systemd/system/
cat /mnt/usr/lib/systemd/system/YaST2-Second-Stage.service 
find / -name YaST2-Second-Stage.service


        ]]></source>
      <chrooted config:type="boolean">false</chrooted>

      </script>
    </chroot-scripts>



